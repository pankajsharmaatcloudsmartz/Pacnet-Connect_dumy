	<entity name="Facility" local-service="true" remote-service="true" table="Facility">
		<column name="id" type="long" primary="true" id-type="identity" db-name="ID" />
		<column name="name" type="String" db-name="Name"/>
		<column name="address" type="String" db-name="Address" />
		<column name="phoneNumber" type="String" db-name="Phone_Number"/>
		<column name="city" type="String" db-name="City"/>
		
		<column name="stateId" type="long" db-name="State_ID" />
		<column name="countryId" type="long" db-name="Country_ID" />
		<column name="isThirdParty" type="int" db-name="Is_Third_Party" />
		<column name="managerId" type="long" db-name="Manager_ID" />
		<column name="escalationEmail" type="String" db-name="Escalation_Email"/>
		<column name="isActive" type="boolean" db-name="IsActive" />
		
		<column name="createdOn" type="Date" db-name="Created_On" />
		<column name="createdBy" type="String" db-name="Created_By" />
		<column name="updatedOn" type="Date" db-name="Updated_On" />
		<column name="updatedBy" type="String" db-name="Updated_By" />
		<column name="timezone" type="String" db-name="timezone" />
		
		<!-- Order -->		
		<finder name="FacilityName" return-type="Collection">
			<finder-column name="name" />
		</finder>
		
		<finder name="FacilityByCountryId" return-type="Collection">
			<finder-column name="countryId" />
		</finder>
		<finder name="FacilityByManagerId" return-type="Collection">
   			<finder-column name="managerId" />
 		 </finder>
	</entity>
	
